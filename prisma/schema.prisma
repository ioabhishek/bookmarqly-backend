generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String  @id @default(cuid())
  name         String
  username     String? @unique @default(cuid())
  email        String  @unique
  password     String?
  picture      String?
  refreshToken String?
}
